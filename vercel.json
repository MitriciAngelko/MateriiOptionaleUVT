{
    "version": 2,
    "functions": {
        "api/**/*.js": {
        "runtime": "vercel/node@22.x"
        }
    },
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/api/server.js"
      },
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ]
}